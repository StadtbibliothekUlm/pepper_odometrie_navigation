        #put clean-up code here
        pass

    def onInput_onStart(self):
        if(self._isTooFar()):
            self.QRSearch()
        else:
            self.QRMove()

    def _isTooFar(self):
        SensorL = self.mem.getData("Device/SubDeviceList/Platform/InfraredSpot/Left/Sensor/Value")
        SensorR = self.mem.getData("Device/SubDeviceList/Platform/InfraredSpot/Right/Sensor/Value")

        self.logger.info("isTooFar -> Sensor: R:"+str(SensorR)+" L:"+str(SensorL))

        LaserL = int((self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Left/Seg01/X/Sensor/Value")*100))
        LaserR = int((self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Right/Seg01/X/Sensor/Value")*100))

        self.logger.info("isTooFar -> Laser: R:"+str(LaserR)+" L:"+str(LaserL))

        if(SensorL == 1.0 and SensorR == 1.0):
            return False
        elif(LaserL < 110 and LaserR < 110):
            return False
        else:
            return True

    def onInput_moveFailed(self):
        global move_failed_counter

        move_failed_counter +=1

        leftX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Left/Seg01/X/Sensor/Value")
        rightX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Right/Seg01/X/Sensor/Value")

        left = int((leftX_raw*10))
        right = int((rightX_raw*10))

        if(move_failed_counter > 3):
            move_failed_counter = 0
            self.turnBack()
        elif(left < 6 or right < 6):
            self.turnBack()
        else:
            self.QRMove()

    def onInput_movefinished(self):
        global move_failed_counter
        move_failed_counter = 0
        self.QRMove()

    def QRMove(self):
        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")
        if(not self._isQRMiddle()):
            self._checkQRMiddle()
        elif(not self._isQRSitesEven()):
            self._checkQRCodeSites()
        else:
            self.onStopped()

    def _checkQRMiddle(self):
        global QRMiddleThreshold
        global Middle

        ValR = Middle + QRMiddleThreshold
        ValL = Middle - QRMiddleThreshold

        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")
        if(QRCodeData):
            time.sleep(1)
            middleX = self._getSquareMiddle(QRCodeData[0][1])
            if(middleX > ValR):
                self.logger.info("Value: "+str(middleX)+" shiftRight")
                time.sleep(1.5)
                self.shiftRight()
            elif(middleX < ValL):
                self.logger.info("Value: "+str(middleX)+" shiftLeft")
                time.sleep(1.5)
                self.shiftLeft()
            else:
                self.logger.info("MiddleVal -> "+str(middleX))
                self.QRMove()
        else:
            self.QRSearch()

    def _checkQRCodeSites(self):
        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")
        if(QRCodeData):
            DistanceYL = QRCodeData[0][1][0][1] - QRCodeData[0][1][1][1]
            DistanceYR = QRCodeData[0][1][2][1] - QRCodeData[0][1][3][1]

            self.logger.info("YL -> "+str(DistanceYL)+" YR -> "+str(DistanceYR))

            Distance = DistanceYR - DistanceYL

            self.logger.info("Distance -> "+str(Distance))

            if(Distance < 100 and Distance > -100):
                self.QRMove()
            elif(DistanceYL < DistanceYR):
                self.turnLeft()
            elif(DistanceYL < DistanceYR):
                self.turnRight()
            else:
                self.QRMove()

    def _getSquareMiddle(self,points):
        middleX = ((points[0][0] + points[2][0]) / 2)
        return middleX

    def _isQRMiddle(self):
        global QRMiddleThreshold
        global Middle

        ValR = Middle + QRMiddleThreshold
        ValL = Middle - QRMiddleThreshold

        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")
        if(QRCodeData):
            middleX = self._getSquareMiddle(QRCodeData[0][1])
            if(middleX < ValR and middleX > ValL):
                return True
            else:
                return False
        else:
            return False

    def _isQRSitesEven(self):
        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")
        if(QRCodeData):
            DistanceYL = QRCodeData[0][1][0][1] - QRCodeData[0][1][1][1]
            DistanceYR = QRCodeData[0][1][2][1] - QRCodeData[0][1][3][1]

            self.logger.info("YL -> "+str(DistanceYL)+" YR -> "+str(DistanceYR))

            Distance = DistanceYL - DistanceYR

            self.logger.info("Distance -> "+str(Distance))

            if(Distance < 100 and Distance > -100):
                return True
            else:
                return False
        else:
            return False

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Input name="movefinished" type="1" type_size="1" nature="1" inner="0" tooltip="" id="4" /><Input name="moveFailed" type="1" type_size="1" nature="1" inner="0" tooltip="" id="5" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="6" /><Output name="turnRight" type="1" type_size="1" nature="2" inner="0" tooltip="" id="7" /><Output name="turnLeft" type="1" type_size="1" nature="2" inner="0" tooltip="" id="8" /><Output name="turnForward" type="1" type_size="1" nature="2" inner="0" tooltip="" id="9" /><Output name="turnBack" type="1" type_size="1" nature="2" inner="0" tooltip="" id="10" /><Output name="shiftLeft" type="1" type_size="1" nature="2" inner="0" tooltip="" id="11" /><Output name="shiftRight" type="1" type_size="1" nature="2" inner="0" tooltip="" id="12" /><Output name="QRSearch" type="1" type_size="1" nature="2" inner="0" tooltip="" id="13" /></Box><Box name="Moves" id="3" localization="8" tooltip="This box is empty and should be used to create any box diagram you would like.&#x0A;&#x0A;To edit its diagram, double-click on it." x="413" y="83"><bitmap>media/images/box/box-diagram.png</bitmap><script language="4"><content><![CDATA[]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id=
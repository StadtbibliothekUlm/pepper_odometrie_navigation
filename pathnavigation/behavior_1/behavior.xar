        self.mem = ALProxy("ALMemory")

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")
        if QRCodeData:
            self.WallPositioning()
        else:
            self.QRSearch()

    def onInput_moveFailed(self):
        global mode

        leftX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Left/Seg01/X/Sensor/Value")
        rightX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Right/Seg01/X/Sensor/Value")

        left = int((leftX_raw*10))
        right = int((rightX_raw*10))

        if(left < 7 or right < 7):
            self.turnBack()
        else:
            if(mode == 1):
                self.WallPositioning()
            else:
                self.checkQRCode()

    def onInput_movefinished(self):
        global mode
        if(mode == 1):
            self.WallPositioning()
        else:
            self.checkQRCode()

    def WallPositioning(self):
        global mode
        global try_counter
        global qrpos_counter

        mode = 1

        self.logger.info("Mode: "+str(mode)+" TryCounter: "+str(try_counter))

        leftX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Left/Seg01/X/Sensor/Value")
        rightX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Right/Seg01/X/Sensor/Value")

        self.logger.info("Left: "+str(leftX_raw)+" Right: "+str(rightX_raw))

        left_int = int((leftX_raw*100))
        right_int = int((rightX_raw*100))

        if(left_int > 150 or right_int > 150):
            self.QRSearch()
        else:
            if(try_counter < 5):
                if(left_int > 100 or right_int > 100):
                    time.sleep(1)
                    self.turnForward()
                elif(left_int == right_int):
                    time.sleep(1)
                    self.checkQRCode()
                elif(left_int > right_int):
                    time.sleep(1)
                    try_counter += 1
                    self.turnLeft()
                elif(left_int < right_int):
                    time.sleep(1)
                    try_counter += 1
                    self.turnRight()
            else:
                self.checkQRCode()

    def checkQRCode(self):
        global mode
        global try_counter
        global qrpos_counter

        mode = 2

        self.logger.info("Mode: "+str(mode)+" TryCounter: "+str(try_counter))

        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")
        if(QRCodeData):
            middleX = self._getSquareMiddle(QRCodeData[0][1])

            if(qrpos_counter > 4):
                try_counter = 0
                qrpos_counter = 0
                self.WallPositioning()
            elif(middleX > 1400):
                self.logger.info("Value: "+str(middleX)+" shiftRight")
                time.sleep(1.5)
                qrpos_counter +=1
                self.shiftRight()
            elif(middleX < 1200):
                self.logger.info("Value: "+str(middleX)+" shiftLeft")
                time.sleep(1.5)
                qrpos_counter +=1
                self.shiftLeft()
            else:
                self.logger.info("Value: "+str(middleX)+" In the middle")
                if(try_counter>4):
                    try_counter = 0
                    qrpos_counter = 0
                    self.WallPositioning()
                else:
                    qrpos_counter = 0
                    self.onStopped()
        else:
            self.QRSearch()

    def _getSquareMiddle(self,points):
        middleX = ((points[0][0] + points[2][0]) / 2)
        return middleX

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Input name="movefinished" type="1" type_size="1" nature="1" inner="0" tooltip="" id="4" /><Input name="moveFailed" type="1" type_size="1" nature="1" inner="0" tooltip="" id="5" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="6" /><Output name="turnRight" type="1" type_size="1" nature="2" inner="0" tooltip="" id="7" /><Output name="turnLeft" type="1" type_size="1" nature="2" inner="0" tooltip="" id="8" /><Output name="turnForward" type="1" type_size="1" nature="2" inner="0" tooltip="" id="9" /><Output name="turnBack" type="1" type_size="1" nature="2" inner="0" tooltip="" id="10" /><Output name="shiftLeft" type="1" type_size="1" nature="2" inner="0" tooltip="" id="11" /><Output name="shiftRight" type="1" type_size="1" nature="2" inner="0" tooltip="" id="12" /><Output name="QRSearch" type="1" type_size="1" nature="2" inner="0" tooltip="" id="13" /></Box><Box name="Moves (1)" id="2" localization="8" tooltip="This box is empty and should be used to create any box diagram you would like.&#x0A;&#x0A;To edit its diagram, double-click on it." x="471" y="70"><bitmap>media/images/box/box-diagram.png</bitmap><script language="4"><content><![CDATA[]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input."
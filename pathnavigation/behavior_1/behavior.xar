        QRCodeData = self.mem.getData("BarcodeReader/BarcodeDetected")

        if QRCodeData:
            middleX = self._getSquareMiddle(QRCodeData[0][1])
            if(middleX > 1400):
                self.logger.info("Value: "+str(middleX)+" shiftRight")
                time.sleep(1.5)
                self.shiftRight()
            elif(middleX < 1200):
                self.logger.info("Value: "+str(middleX)+" shiftLeft")
                time.sleep(1.5)
                self.shiftLeft()
            else:
                self.logger.info("Value: "+str(middleX)+" In the middle")
                self.reachedWall()
        else:
            self.QRSearch()

    def WallPositioning(self):
        global move
        move = 2;
        leftX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Left/Seg01/X/Sensor/Value")
        rightX_raw = self.mem.getData("Device/SubDeviceList/Platform/LaserSensor/Front/Vertical/Right/Seg01/X/Sensor/Value")

        self.logger.info("Left: "+str(leftX_raw)+" Right: "+str(rightX_raw))

        left_int = int((leftX_raw*100))
        right_int = int((rightX_raw*100))

        leftX = "%.5f" % leftX_raw
        rightX = "%.5f" % rightX_raw

        if(left_int == right_int):
            time.sleep(1)
            if(left_int > 100):
                self.turnForward()
            else:
                self.checkQRCode()
        elif(left_int > right_int):
            time.sleep(1)
            self.turnLeft()
        elif(left_int < right_int):
            time.sleep(1)
            self.turnRight()

    def _getSquareMiddle(self,points):
        middleX = ((points[0][0] + points[2][0]) / 2)
        return middleX

